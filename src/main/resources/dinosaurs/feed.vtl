<h2 class="pageHeader">Dino Menu</h2>

<form action="/dinosaurs/$dinosaur.getId()/feed" method="post">

        <tr>
            <label for="menu">Please Select:</label>
            <select name="food" id="menu">
            #foreach($item in $food)
                <option value="$item.getName()">$item.getName()</option>
            #end
            </select>
            <input type="submit", value="Enter">
        </tr>
</form>